{
  "name": "{{name}}",
  "title": "{{title}}",
  {{#if description}}
  "description": "{{description}}",
  {{/if}}
  "version": "{{version}}",
  "class": "{{class}}",
  "prefix": "{{prefix}}",
  "homepage": "{{homepage}}",
  "author": "{{author}}",
  "license": "{{license}}",
  "style": "{{name}}.css",
  "count": "{{count}}",
  "classifiable": {{classifiable}},
  "icons": {
    {{#each icons}}
    "{{name}}": "{{title}}",
    {{/each}}
  }
  {{#if classifiable}}
  ,
  "categories": {
    {{#each categories}}
    "{{@key}}": [
      {{#each this}}
      "{{this}}",
      {{/each}}
    ],
    {{/each}}
  }
  {{/if}}
}

